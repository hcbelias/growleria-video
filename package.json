{
  "name": "your-project-name",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "set NODE_ENV=development && npm run server",
    "clean": "rimraf dist",
    "build": "npm run clean && babel ./src --out-dir dist",
    "prod": "set NODE_ENV=production && npm run build && npm run server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "composable-middleware": "^0.3.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "nodemon": "^1.18.9",
    "rimraf": "^2.7.1"
  }
}
